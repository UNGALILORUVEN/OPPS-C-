            #include <iostream>
            using namespace std;
            int main() {int water,it,ft;
                        float Q;
                        cin>>water>>it>>ft;
                        Q=water*(ft-it)*4184;
                        cout<<"The energy needed is "<<Q;
                       return 0;
            }

**2.)SCIENTIST GAME**

            #include <iostream>
            using namespace std;

            int main()
            {
                int num, r, sum = 0, temp;
                cin >> num;
                for (temp = num; num != 0; num = num / 10) 
                        {
                    r = num % 10;
                    sum = sum + (r * r * r);
                }
                if (sum == temp)
                    cout << "Give to Scientist Armstrong" << endl;
                else
                    cout <<"Dont Give to Scientist Armstrong" << endl;
            }
